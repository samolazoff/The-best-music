<section class="add-misic">
    <h2 class="block__title"><%= title  %></h2>
    <form action="/music/edit" class="col s12" method="post">
        <div class="row">
            <div class="input-field col s12">
                <input id="album" type="text" class="validate" name="album" value=<%= music.album  %>>
                <label for="album">Album</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input id="artist" type="text" class="validate" name="artist" value=<%= music.artist  %>>
                <label for="artist">Artist</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input id="img" type="text" class="validate" name="img" value=<%= music.urlImg  %>>
                <label for="img">Image album</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input id="description" type="text" class="validate" name="description" value=<%= music.urlDescription  %>>
                <label for="description">Description of album</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <input id="link" type="text" class="validate" name="link" value=<%= music.urlWiki  %>>
                <label for="link">Link Wiki of album</label>
            </div>
        </div>
        <div class="input-field col s12">
            <input id="musicId" type="hidden" name="musicId" value="<%= music.id%>">
        </div>
        <button class="btn waves-effect waves-light" type="submit" name="action">Submit
            <i class="material-icons right">send</i>
        </button
    </form>
    
</section>